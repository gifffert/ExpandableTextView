<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#273833</color>
    <color name="colorAccent">#d81b60</color>

    <color name="colorPrimaryTextDefaultMaterialLight">#DE000000</color>
    <color name="colorSecondaryTextDefaultMaterialLight">#8A000000</color>

    <color name="colorPrimaryTextDefaultMaterialDark">#FFFFFFFF</color>
    <color name="colorSecondaryTextDefaultMaterialDark">#CCFFFFFF</color>
    <color name="gray">#46000000</color>
    <string name="app_name">ExpandableTextView</string>
    <declare-styleable name="ExpandTextView">
        <attr format="color" name="textColor"/>
        <attr format="color" name="primaryTextColor"/>
        <attr format="string" name="collapseText"/>
        <attr format="string" name="expandText"/>
        <attr format="dimension" name="textSize"/>
        <attr format="reference" name="collapsedBitmap"/>
        <attr format="reference" name="expandedBitmap"/>
        <attr format="integer" name="expandedLines"/>
        <attr format="integer" name="timeAnimation"/>
        <attr format="boolean" name="showHideLine"/>
    </declare-styleable>
</resources>
